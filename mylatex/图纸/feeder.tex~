\draw (M) ++(120:1) node [contact,name=U]{};
\draw (M) ++(90:1) node [contact,name=V]{};
\draw (M) ++(60:1) node [contact,name=W]{};


\draw (V) -- node[left] {V} (V1)
--++(0,1)
to [thermic sensor] ++(0,1)
